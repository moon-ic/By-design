{
  "name": "moonic-design",
  "version": "0.0.9",
  "description": "基于react的组件库",
  "type": "module",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/moon-ic/moonic-design"
  },
  "scripts": {
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "clean": "rimraf ./dist",
    "build": "pnpm clean && pnpm build:ts && pnpm build:css",
    "build:ts": "tsc -p tsconfig.build.json",
    "build:css": "sass ./src/styles/index.scss ./dist/index.css",
    "build:storybook": "build-storybook -s public",
    "lint": "eslint --fix --quiet ./packages",
    "storybook": "storybook dev -p 6006"
  },
  "lint-staged": {
    "*.(js|jsx|ts|tsx)": [
      "eslint --fix --quiet",
      "git add"
    ]
  },
  "devDependencies": {
    "@chromatic-com/storybook": "1.9.0",
